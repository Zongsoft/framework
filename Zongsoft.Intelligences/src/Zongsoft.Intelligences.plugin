<?xml version="1.0" encoding="utf-8" ?>

<plugin name="Zongsoft.Intelligences"
        title="Zongsoft Intelligences Plugin"
        author="Zongsoft Studio(zongsoft@qq.com)"
        copyright="Copyright(C) Zongsoft Studio. 2025-2025. All rights reserved."
        description="Created Date: 2025-08-08 by Popeye Zhong">

	<manifest>
		<assemblies>
			<assembly name="Zongsoft.Intelligences" />
		</assemblies>
	</manifest>

	<extension path="/Workbench/Executor/Commands">
		<object name="Ollama">
			<ollama.command>
				<object name="Ollama" type="Zongsoft.Intelligences.Commands.OllamaCommand, Zongsoft.Intelligences" />
			</ollama.command>

			<object name="Chat" type="Zongsoft.Intelligences.Commands.ChatCommand, Zongsoft.Intelligences" />
			<object name="List" type="Zongsoft.Intelligences.Commands.OllamaListCommand, Zongsoft.Intelligences" />

			<object name="History">
				<history.command>
					<object name="History" type="Zongsoft.Intelligences.Commands.HistoryCommand, Zongsoft.Intelligences" />
				</history.command>

				<object name="Clear" type="Zongsoft.Intelligences.Commands.HistoryClearCommand, Zongsoft.Intelligences" />
			</object>
		</object>
	</extension>
</plugin>
