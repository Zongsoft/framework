<?xml version="1.0" encoding="utf-8" ?>

<configuration>
	<option path="/Data">
		<connectionSettings>
			<connectionSetting connectionSetting.name="db2" driver="MySQL" mode="all" value="server=192.168.0.1" />
		</connectionSettings>
	</option>

	<option path="/Web/OpenAPI/Authentication">
		<authenticator authenticator.name="bearer" kind="http" scheme="Bearer" />
		<authenticator authenticator.name="basic" kind="http" scheme="Basic">
			<username>administrator</username>
			<password>xxxxxx</password>
		</authenticator>
	</option>

	<option path="/Messaging/Queues">
		<queue queue.name="avm" timeout="30s">
			<subscription reliability="MostOnce">
				<topic topic.name="heartbeat" />

				<topic topic.name="uplink">
					<tag />
					<tag>ping</tag>
					<tag>synchronize</tag>
				</topic>

				<topic topic.name="downlink">
					<tag></tag>
					<tag>control</tag>
					<tag>synchronize</tag>
				</topic>
			</subscription>
		</queue>
	</option>

	<option path="/Settings">
		<setting setting.name="Setting1" value="SettingValue#1" />
		<setting setting.name="Setting2" value="SettingValue#2" />
	</option>
</configuration>
